set -x
Usage() {
	echo K C E ?
}
if test -z "$1"; then Usage; exit 1; fi
if test -z "$2"; then Usage; exit 1; fi
if test -z "$3"; then Usage; exit 1; fi
python3 BlackOA.py \
	-M Fashion -D cuda -A NES \
	-e $(dc << EOF
7 k $3 255 / p
EOF
) \
	-N 10000 -k "$1" -c "$2" -P 1

# NES_NPOP=100 NES_XLR=1 NES_XSIGMA=3 ./bin/nes 5 -1 4
